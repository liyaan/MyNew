<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <Button
        android:id="@+id/start_download"
        android:layout_width="match_parent"
        android:layout_height="50dip"
        android:text="开始"></Button>
    <Button
        android:id="@+id/pause_download"
        android:layout_width="match_parent"
        android:layout_height="50dip"
        android:text="暂停"></Button>
    <Button
        android:id="@+id/cancel_download"
        android:layout_width="match_parent"
        android:layout_height="50dip"
        android:text="取消"></Button>
    <Button
        android:id="@+id/open_weacher"
        android:layout_width="match_parent"
        android:layout_height="50dip"
        android:text="打开微信"></Button>
    <Button
        android:id="@+id/click_btn_text"
        android:layout_width="match_parent"
        android:layout_height="50dip"
        android:text="点击"></Button>
    <Button
        android:id="@+id/click_intent_service"
        android:layout_width="match_parent"
        android:layout_height="50dip"
        android:text="intentService"
        />
</androidx.appcompat.widget.LinearLayoutCompat>